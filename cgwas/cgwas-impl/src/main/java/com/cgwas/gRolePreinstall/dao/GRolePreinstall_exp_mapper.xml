<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<!--@author yubangqiong  --> 
<mapper namespace="com.cgwas.gRolePreinstall.dao">
	<sql id="columnsAs">
		t1.id, t1.role_name
	</sql>

	<select id="selectGRolePreinstallPage" resultType="com.cgwas.gRolePreinstall.entity.GRolePreinstallVo">
		select
		<include refid="columnsAs"/>
		from
		p_g_role_preinstall t1
		join
		(SELECT
		id
		from
		p_g_role_preinstall
		where 1=1
		
		<include refid="conds" />
		order by id desc
		limit #{firstrownum}, #{limit}) t2
		where t1.id = t2.id
	</select>
	
	<select id="selectGRolePreinstallCount" resultType="java.lang.Long">
		select
		count(id)
		from p_g_role_preinstall
		where 1=1
		<include refid="conds" />
	</select>

</mapper>