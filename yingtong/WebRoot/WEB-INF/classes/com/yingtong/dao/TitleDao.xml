<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.yingtong.dao.TitleDao">
	  <select id="findAllTitles" resultType="Title">
	  select * from yt_title
	  </select>
	  <select id="findTitleById" resultType="Title">
	  select * from yt_title where id=#{id}
	  </select>
</mapper>